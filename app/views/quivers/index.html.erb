<p id="notice"><%= notice %></p>

<h1>Listing Quivers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quivers.each do |quiver| %>
      <tr>
        <td><%= link_to 'Show', quiver %></td>
        <td><%= link_to 'Edit', edit_quiver_path(quiver) %></td>
        <td><%= link_to 'Destroy', quiver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiver', new_quiver_path %>
