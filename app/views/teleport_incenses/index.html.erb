<p id="notice"><%= notice %></p>

<h1>Listing Teleport Incenses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teleport_incenses.each do |teleport_incense| %>
      <tr>
        <td><%= link_to 'Show', teleport_incense %></td>
        <td><%= link_to 'Edit', edit_teleport_incense_path(teleport_incense) %></td>
        <td><%= link_to 'Destroy', teleport_incense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

