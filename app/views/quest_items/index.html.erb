<p id="notice"><%= notice %></p>

<h1>Listing Quest Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quest_items.each do |quest_item| %>
      <tr>
        <td><%= link_to 'Show', quest_item %></td>
        <td><%= link_to 'Edit', edit_quest_item_path(quest_item) %></td>
        <td><%= link_to 'Destroy', quest_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quest item', new_quest_item_path %>
