<p id="notice"><%= notice %></p>

<h1>Listing Elf Wings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elf_wings.each do |elf_wing| %>
      <tr>
        <td><%= link_to 'Show', elf_wing %></td>
        <td><%= link_to 'Edit', edit_elf_wing_path(elf_wing) %></td>
        <td><%= link_to 'Destroy', elf_wing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elf wing', new_elf_wing_path %>
