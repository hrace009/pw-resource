<p id="notice"><%= notice %></p>

<h1>Listing Resurrection Scrolls</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resurrection_scrolls.each do |resurrection_scroll| %>
      <tr>
        <td><%= link_to 'Show', resurrection_scroll %></td>
        <td><%= link_to 'Edit', edit_resurrection_scroll_path(resurrection_scroll) %></td>
        <td><%= link_to 'Destroy', resurrection_scroll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resurrection scroll', new_resurrection_scroll_path %>
