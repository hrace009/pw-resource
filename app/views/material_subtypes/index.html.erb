<p id="notice"><%= notice %></p>

<h1>Listing Material Subtypes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @material_subtypes.each do |material_subtype| %>
      <tr>
        <td><%= link_to 'Show', material_subtype %></td>
        <td><%= link_to 'Edit', edit_material_subtype_path(material_subtype) %></td>
        <td><%= link_to 'Destroy', material_subtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Material subtype', new_material_subtype_path %>
