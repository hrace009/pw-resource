<p id="notice"><%= notice %></p>

<h1>Listing Chi Stones</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chi_stones.each do |chi_stone| %>
      <tr>
        <td><%= link_to 'Show', chi_stone %></td>
        <td><%= link_to 'Edit', edit_chi_stone_path(chi_stone) %></td>
        <td><%= link_to 'Destroy', chi_stone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chi stone', new_chi_stone_path %>
